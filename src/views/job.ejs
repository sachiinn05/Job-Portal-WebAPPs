<% jobs.forEach(job=>{ %>
  <div class="card">
    <div class="card-header">
          <h1 class="company"><%=job.companyName%></h1>
          <h5 class="designation"><%=job.designation%></h5>
        </div>
        <div class="card-body">
          <h5 class="card-title location">
            <svg xmlns="http://www.w3.org/2000/svg" height="20" width="15" viewBox="0 0 384 512">
            <%=job.location%>
          </h5>
          <div class="salary">
            <h5>
            <svg xmlns="http://www.w3.org/2000/svg" height="20" width="12.5" viewBox="0 0 320 512">
            <%=job.salary%></h5>
          </div>
          <br>
          <div class="skills">
            <%job.skills.forEach(skill=>{ %>
              <button type="button" class="btn btn-outline-info"><%=skill%></button>
            <% }) %>
          </div>
          <br>
          <div>
            <a href="/job-details/<%=job.id%>" class="btn btn-outline-success d-grid gap-2">View Details</a>
          </div>
        </div>
      </div>
    <% }) %>